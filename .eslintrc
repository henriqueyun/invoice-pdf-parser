{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es2021": true
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}